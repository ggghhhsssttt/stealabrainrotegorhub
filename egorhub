╛я╛╛я┐пя╛┐я╛пя┐пя╛╛я╛┐я┐пя╛╛я╛пя┐пя╛┐я╛пя┐пя╛╛я╛╛я┐пя╛╛я╛┐я┐пя╛┐я╛пя┐пя╛╛я╛╛я┐пя╛╛я╛пя┐пя╛┐я╛пя┐пя╛╛я╛┐я┐пя╛╛я╛пя┐пя╛┐я╛пя┐пя╛╛я╛╛я┐пя╛╛я╛╛я┐пя╛┐я╛пя┐пя╛╛я╛╛я┐пя╛╛я╛╛я┐пя╛┐я╛пя┐пя╛╛я╛┐я┐пя╛╛я╛пя┐пя╛┐я╛пя┐пя╛╛я╛╛я┐пя╛╛я╛╛я┐пя╛┐я╛пя┐пя╛╛я╛╛я┐пя╛╛я╛И ServerHop Button (with Surround Frame)
-- Da Sync (one-time execute button)
local daSyncBtn = Instance.new("TextButton")
daSyncBtn.Size = UDim2.new(1, -10, 0, 40)
daSyncBtn.BackgroundColor3 = Color3.fromRGB(120, 120, 120)
daSyncBtn.Text = "Desync (lag for other players)"
daSyncBtn.TextColor3 = Color3.new(1, 1, 1)
daSyncBtn.Font = Enum.Font.SourceSans
daSyncBtn.TextSize = 20
daSyncBtn.Parent = scroll

local daSyncCorner = Instance.new("UICorner")
daSyncCorner.CornerRadius = UDim.new(0, 8)
daSyncCorner.Parent = daSyncBtn

-- Skibidi list (as provided)
local function skibidiList()
    pcall(function()
        setfflag("WorldStepMax", "-99999999999999")
        setfflag("FIntFlagUpdateVersion", "132")
        setfflag("DFIntTaskSchedulerTargetFps", "9999")
        setfflag("FFlagHandleAltEnterFullscreenManually", "false")
        setfflag("FFlagTaskSchedulerLimitTargetFpsTo2402", "false")
        setfflag("FIntInterpolationAwareTargetTimeLerpHundredth", "10")
        setfflag("DFIntMaxFrameBufferSize", "2")
        setfflag("DFIntMaxFramesToSend", "1")
        setfflag("DFIntDebugSendDistInSteps", "-21474836483757393753797957957359739759755976895695695979759997949459748874978489458745894")
        setfflag("DFIntMaxAverageFrameDelayExceedFactor", "0")
        setfflag("DFFlagPlayerHumanoidPropertyUpdateRestrict", "True")
        setfflag("DFIntMaxMissedWorldStepsRemembered", "-21474836483757393753797957957359739759755976895695695979759997949459748874978489458745894")
        setfflag("DFIntDebugDefaultTargetWorldStepsPerFrame", "-21474836483757393753797957957359739759755976895695695979759997949459748874978489458745894")
    end)
end

daSyncBtn.MouseButton1Click:Connect(function()
    -- visual feedback
    daSyncBtn.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
    daSyncBtn.Text = "Running Desync..."

    task.spawn(function()
        local player = game:GetService("Players").LocalPlayer
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local char = player.Character or player.CharacterAdded:Wait()
        local hum = char:FindFirstChildOfClass("Humanoid") or char:WaitForChild("Humanoid")
        local backpack = player:WaitForChild("Backpack")

        -- Step 1: initial Skibidi List
        skibidiList()

        -- Step 2: Equip Quantum Cloner
        local function tryEquipQuantumCloner(timeout)
            timeout = timeout or 3
            local start = tick()
            while tick() - start <= timeout do
                -- find tool either in backpack or character
                local qc = backpack:FindFirstChild("Quantum Cloner") or char:FindFirstChild("Quantum Cloner")
                if qc and hum then
                    -- move to character if needed and try equip
                    if qc.Parent ~= char then
                        qc.Parent = char
                    end
                    pcall(function() hum:EquipTool(qc) end)
                    -- wait briefly and verify equipped
                    task.wait(0.25)
                    local equipped = false
                    -- check whether humanoid currently has the tool (Tools appear under character)
                    for _, v in ipairs(char:GetChildren()) do
                        if v:IsA("Tool") and v.Name == "Quantum Cloner" then
                            equipped = true
                            break
                        end
                    end
                    if equipped then return true end
                end
                task.wait(0.1)
            end
            return false
        end

        local equipped = tryEquipQuantumCloner(3)
        if not equipped then
            -- still continue but warn
            warn("Da Sync: Quantum Cloner not equipped within timeout.")
        end

        -- Step 3: wait 1 second before first remote
        task.wait(1)

        -- Step 4: Remote 1 -> RE/UseItem
        local success1, err1 = pcall(function()
            local packages = ReplicatedStorage:FindFirstChild("Packages")
            local netFolder = packages and packages:FindFirstChild("Net")
            local REUseItem = netFolder and (netFolder:FindFirstChild("RE/UseItem") or netFolder:FindFirstChild("RE\\UseItem") )
            if REUseItem and typeof(REUseItem.FireServer) == "function" then
                REUseItem:FireServer()
            else
                warn("Da Sync: RE/UseItem not found.")
            end
        end)
        if not success1 then warn("Da Sync remote1 error:", err1) end

        -- Step 5: Skibidi list again
        skibidiList()
        skibidiList()

        -- Step 6: wait 0.5s
        task.wait(0.5)

        -- Step 7: Remote 2 -> RE/QuantumCloner/OnTeleport
        local success2, err2 = pcall(function()
            local packages = ReplicatedStorage:FindFirstChild("Packages")
            local netFolder = packages and packages:FindFirstChild("Net")
            local REQuantumClonerOnTeleport = netFolder and (netFolder:FindFirstChild("RE/QuantumCloner/OnTeleport") or netFolder:FindFirstChild("RE\\QuantumCloner\\OnTeleport"))
            if REQuantumClonerOnTeleport and typeof(REQuantumClonerOnTeleport.FireServer) == "function" then
                REQuantumClonerOnTeleport:FireServer()
            else
                warn("Da Sync: RE/QuantumCloner/OnTeleport not found.")
            end
        end)
        if not success2 then warn("Da Sync remote2 error:", err2) end

        -- Step 8: final Skibidi list
        skibidiList()
        skibidiList()

        -- final UI feedback
        daSyncBtn.Text = "Desync (Done)"
        task.wait(1.5)
        daSyncBtn.BackgroundColor3 = Color3.fromRGB(120, 120, 120)
        daSyncBtn.Text = "Desync"
    end)
end)



-- Disable / Invert Skibidi List button
local disableSkibidiBtn = makeToggleButton("Disable Desync (reset to work)", function(state)
    if state then
        -- Inverted Skibidi List
    setfflag("WorldStepMax", "4")
    setfflag("FIntFlagUpdateVersion", "132")
